<script>
  jQuery(document).ready(function($) {
    $('.two-wrap .half').click(function(event) {
      /* Act on the event */
      $('.two-wrap .half').removeClass('active');
      $(this).addClass('active')
      var id= $(this).attr('data');

      $('.lrc').removeClass('active');
      $('.lrc.'+id).addClass('active');

    });

  });
</script>
{% schema %}
  {
    "name": "Masonry",
    "class": "index-section",
    "max_blocks": 2,
    "settings": [
      {
        "type": "text",
        "id": "title",
        "label": "Heading",
        "default": "This is your moment"
      },
      {
        "type": "text",
        "id": "l_title",
        "label": "Left Col title",
        "default": "Posts"
      },
      {
        "type": "text",
        "id": "r_title",
        "label": "Right Col title",
        "default": "Filters"
      },
      {
        "type": "text",
        "id": "button_text",
        "label": "Button Text"
      },
      {
        "type": "url",
        "id": "button_url",
        "label": "Button URL"
      }
    ],
    "blocks": [
      {
        "type": "leftcol",
        "name": "Left Col",
        "settings": [
          {
            "type": "image_picker",
            "id": "image_1",
            "label": "Image 1",
            "info": "Recommended Image size 542px width and hight any"
          },
          {
            "type": "url",
            "id": "link_1",
            "label": "Link 1"
          },
          {
            "type": "image_picker",
            "id": "image_2",
            "label": "Image 2"
          },
          {
            "type": "url",
            "id": "link_2",
            "label": "Link 2"
          },
          {
            "type": "image_picker",
            "id": "image_3",
            "label": "Image 3"
          },
          {
            "type": "url",
            "id": "link_3",
